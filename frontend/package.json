    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.0",
    "resize-observer-polyfill": "^1.5.1",
    "ua-parser-js": "^1.0.37",
    "uuid": "^9.0.1",
    "vitest": "^1.3"
  },
