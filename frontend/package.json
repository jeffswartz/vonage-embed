{
  "name": "frontend",
  "private": true,
  "description": "React reference application for the Vonage Video web client SDK.",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "cp-build": "mkdir -p ../backend/dist && cp -r dist ../backend",
    "build": "vite build && yarn cp-build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "ts-check": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/base": "^5.0.0-beta.40",
    "@mui/icons-material": "^5.15.20",
    "@mui/joy": "^5.0.0-beta.36",
    "@mui/material": "^5.15.20",
    "@opentok/client": "^2.28.1",
    "@rollup/plugin-replace": "^5.0.7",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^15.0.5",
    "@testing-library/user-event": "^14.5.1",
    "@vonage/vcr-sdk": "^1.3.0",
    "axios": "^1.7.4",
    "events": "^3.3.0",
    "howler": "^2.2.4",
    "lodash": "^4.17.21",
    "opentok-layout-js": "^5.4.0",
    "opentok-solutions-logging": "^1.1.5",
