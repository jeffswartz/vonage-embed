    "@types/lodash": "^4.17.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.2.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.3.0",
    "@vitest/ui": "^1.3",
    "autoprefixer": "^10.4.19",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.38",
